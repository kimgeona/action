# 실행할 명령어들을 입력하세요

$ echo 실행할 쉘 스크립트 0

:install
	$ echo 실행할 쉘 스크립트 1
	$ echo 실행할 쉘 스크립트 2
	:opencl
		$ echo 실행할 쉘 스크립트 3
	:opencv
		$ echo 실행할 쉘 스크립트 4
		$ echo 실행할 쉘 스크립트 5
		$ echo 실행할 쉘 스크립트 6
	:opengl
		$ echo 실행할 쉘 스크립트 7
		$ echo 실행할 쉘 스크립트 8
		$ echo 실행할 쉘 스크립트 9
		$ echo 실행할 쉘 스크립트 10
		$ echo 실행할 쉘 스크립트 11
    :help
	:gtkmm4
		$ echo 실행할 쉘 스크립트 12
		$ echo 실행할 쉘 스크립트 13
		$ echo 실행할 쉘 스크립트 14
		$ echo 실행할 쉘 스크립트 15
	$ echo 실행할 쉘 스크립트 16
	$ echo 실행할 쉘 스크립트 17

$ echo 실행할 쉘 스크립트 18
$ echo 실행할 쉘 스크립트 19

:uninstall
    $ echo 실행할 쉘 스크립트 20
    $ echo 실행할 쉘 스크립트 21
    $ echo 실행할 쉘 스크립트 22
    :opencv
		$ echo 실행할 쉘 스크립트 23
		$ echo 실행할 쉘 스크립트 24
	:opengl
		$ echo 실행할 쉘 스크립트 25
		$ echo 실행할 쉘 스크립트 26
		$ echo 실행할 쉘 스크립트 27
    $ echo 실행할 쉘 스크립트 28
    $ echo 실행할 쉘 스크립트 29

$ echo 실행할 쉘 스크립트 30
$ echo 실행할 쉘 스크립트 31

